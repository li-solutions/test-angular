{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    },
    {
      "src": "src/server.ts",
      "use": "@vercel/node",
      "config": {
        "distDir": "dist/test-angular/server"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/test-angular/server"
    }
  ]
}
